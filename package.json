{
  "name": "fast-markets",
  "private": true,
  "version": "1.0.0",
  "description": "1 minute markets",
  "main": "index.js",
  "scripts": {
    "sloc": "sloc src/js/",
    "slow-deps": "slow-deps",
    "lint": "eslint src/js/**",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "doc": "esdoc -c .esdocrc",
    "validate": "npm-run-all --parallel validate-webpack:* test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "build": "webpack --env.dev --watch",
    "build:prod": "webpack --env.prod",
    "clean:dist": "rimraf dist",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.dev --inline --progress --hot"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "esdoc": "^0.4.8",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.2",
    "flow-bin": "^0.30.0",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "git@github.com:gotwarlost/istanbul.git",
    "istanbul-instrumenter-loader": "git@github.com:jimenglish81/istanbul-instrumenter-loader.git",
    "jsdom": "^9.5.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon-chai": "^1.2.3",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.0",
    "npm-run-all": "^2.3.0",
    "opt-cli": "^1.5.1",
    "react-addons-test-utils": "^15.3.1",
