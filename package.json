{
  "name": "fast-markets",
  "private": true,
  "version": "1.0.0",
  "description": "fast markets",
  "main": "index.js",
  "scripts": {
    "sloc": "sloc src/js/",
    "slow-deps": "slow-deps",
    "lint": "eslint src/js/**",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "doc": "esdoc -c .esdocrc",
    "validate": "npm-run-all --parallel validate-webpack:* test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "build": "BABEL_ENV=production webpack --env.dev --watch",
    "build:prod": "npm run clean:dist && webpack --env.prod",
    "clean:dist": "rimraf dist",
    "test": "BABEL_ENV=test mocha --compilers js:babel-register --reporter spec 'tests/**/*.test.js'",
    "test:watch": "npm test -- --watch --inspect",
    "test:coverage": "BABEL_ENV=production istanbul cover _mocha -- --compilers js:babel-register --colors --reporter spec 'tests/**/*.test.js'",
    "start": "BABEL_ENV=production webpack-dashboard -c cyan -- webpack-dev-server --env.dev --hot"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "repository": {
    "url": "https://github.com/jimenglish81/fast-markets.git",
    "type": "git"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "esdoc": "^0.4.8",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.2",
    "fetch-mock": "^5.5.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.30.0",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "^9.5.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.0",
    "npm-run-all": "^2.3.0",
    "opt-cli": "^1.5.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "sloc": "^0.1.10",
    "style-loader": "^0.13.1",
    "wallaby-webpack": "0.0.22",
    "webpack": "^2.1.0-beta.21",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-validator": "^2.2.6"
  },
  "dependencies": {
    "d3": "4.2.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.14.1",
    "moment": "^2.15.1",
    "numeral": "^1.5.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-faux-dom": "^2.7.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.3",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  }
}
